{
  "extends": ["airbnb", "plugin:react/recommended", "prettier"],
  "env": {
    "es6": true,
    "node": true,
    "browser": false,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
